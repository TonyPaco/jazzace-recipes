<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of Pano2VR and creates a package.</string>
	<key>Identifier</key>
	<string>com.github.jazzace.pkg.Pano2VR</string>
	<key>Input</key>
	<dict/>
	<key>MinimumVersion</key>
	<string>1.4</string>
	<key>ParentRecipe</key>
	<string>com.github.jazzace.download.Pano2VR</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>AppPkgCreator</string>
			<key>Arguments</key>
			<dict>
				<key>Comment</key>
				<string>forces version gleaned from Sparkle feed, as app version currently includes a / and a build number</string>
				<key>version</key>
				<string>%version%</string>
			</dict>
		</dict>
	</array>
</dict>
</plist>
