<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest version of TouchOSC Editor.</string>
    <key>Identifier</key>
    <string>com.github.jazzace.download.TouchOSCEditor</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>TouchOSC Editor</string>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.9</string>
    <key>Process</key>
    <array>
       <dict>
            <key>Processor</key>
            <string>URLTextSearcher</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>http://hexler.net/software/touchosc</string>
                <key>re_pattern</key>
                <string>(?P&lt;uri&gt;/pub/touchosc/touchosc-editor-(?P&lt;version&gt;.*?)-osx\.zip)</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <string>http://www.hexler.net%uri%</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
    </array>
</dict>
</plist>